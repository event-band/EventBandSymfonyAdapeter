{
    "name": "event-band/symfony-adapter",
    "description": "Asynchronous event dispatching",
    "keywords": ["event", "dispatcher", "queue", "async", "asynchronous", "message"],
    "homepage": "http://github.com/chEbba/EventBandAdapterSymfony",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill chEbba Chebunin",
            "email": "iam@chebba.org",
            "homepage": "http://github.com/chEbba"
        }
    ],

    "autoload": {
        "psr-0": { "EventBand\\Adapter\\Symfony": "src/" }
    },

    "minimum-stability": "dev",

    "require": {
        "php": ">= 5.4",
        "event-band/band-framework": "dev-master",
        "symfony/event-dispatcher": "~2.7",
        "symfony/console": "~2.7",
        "che/console-signals": "dev-master"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/event-dispatcher": "2.7.*",
        "symfony/console": "2.7.*"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    }
}
